#Configure Speed/Duplex
$physnics = get-vmhost $esxHost | get-vmhostnetwork | % {$_.Physicalnic}
set-vmhostnetworkadapter -PhysicalNic $physnics -BitRatePerSecMB 1000 -Duplex full

