#ESX Host Cluster Preparation Script

#Variables
$VC = Read-Host "Which VC would you like to connect to?: (ex: vcenter-prod, vcenter-lab, vcenter-view or vcenter-dr) "
$esxHost = Read-Host "Which ESX host would you like to configure? (ex: FQDN - usemvsh01.us.kworld.kpmg.com) "
$Env = Read-Host "Which environment are you building? (ex: dev, prod, lab, dr, dmz, build or view): "
#$timeZone = Read-Host "Which time zone are you in? (ex: America/New_York, America/Chicago): "
#$timeZone = "America/New_York"
#$hostType = Read-Host "What type of host are you building (ex: g1, g2, g5, etc..)?: "
$vcUser = Read-Host "User ID: "
$vcPassword = Read-Host "Password: "

Connect-VIServer $VC -User $VCUser -Password $VCPassword

